{
  "name": "billing-app",
  "version": "0.0.0",
  "description": "Billing app test for sky",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "dev": "NODE_ENV=development npm start",
    "start": "node devServer.js",
    "lint": "standard",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "static": "node_modules/.bin/http-server docs/ -p 3000",
    "e2e:prod": "npm run build && ./node_modules/.bin/concurrently -k 'npm run static' 'npm run e2e'",
    "postinstall": "node nightwatch.conf.js",
    "test": "NODE_ENV=test mocha 'src/**/*Spec.js' --compilers js:babel-register --recursive --require ./test/setup.js --require ./test/helpers.js",
    "test:watch": "npm test -- --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nim23/billing-app.git"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "fetch",
      "afterEach"
    ]
  },
  "license": "MIT",
  "homepage": "https://github.com/nim23/billing-app.git",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^6.3.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.5.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "mocha": "^3.1.0",
    "nightwatch": "^0.9.8",
    "nock": "^8.0.0",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.3.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.4.0",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "selenium-download": "^2.0.6",
    "standard": "^8.4.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "wallaby-webpack": "0.0.25",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux-thunk": "^2.1.0"
  }
}
